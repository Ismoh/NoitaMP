{
  "Lua.workspace.library": [".debug/noita_api.lua", "${3rd}/love2d/library"],
  "Lua.workspace.maxPreload": 2000,
  "cmake.configureOnOpen": false,
  "Lua.diagnostics.globals": [
    "describe",
    "it",
    "jit",
    "bitser",
    "MOD_SETTING_SCOPE_RUNTIME",
    "MOD_SETTING_SCOPE_RUNTIME_RESTART",
    "mod_settings_get_version",
    "mod_settings_update",
    "mod_settings_gui_count",
    "mod_settings_gui",
    "mod_setting_change_callback",
    "mod_settings_version",
    "mod_settings",
    "draw_gui",
    "mod_setting_readonly",
    "mod_setting_get_id",
    "mod_setting_tooltip",
    "ngx",
    "EXPORT_ASSERT_TO_GLOBALS",
    "gui"
  ],
  "lua.targetVersion": "5.1",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "Lua.runtime.version": "LuaJIT",
  "Lua.workspace.checkThirdParty": false,
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  }
}
